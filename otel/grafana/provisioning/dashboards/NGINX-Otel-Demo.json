          },
          "limit": 100,
          "metricsQueryType": "range",
          "query": "{\n\tspan.client.geo.country.name != \"\" && \n\tspan.client.geo.city != \"\" &&\n\tspan.client.geo.lat != \"\" && \n\tspan.client.geo.lon != \"\" && \n\tspan.http.client_ip != \"\"\n}",
          "queryType": "traceql",
          "refId": "A",
          "serviceMapUseNativeHistograms": true,
          "tableType": "spans"
        }
      ],
      "title": "Geomap",
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "byVariable": false,
            "include": {
              "names": [
                "client.geo.country.name",
                "client.geo.lat",
                "client.geo.lon",
                "Duration",
                "client.geo.city",
                "http.client_ip"
              ]
            }
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Duration": {
                "aggregations": [
                  "max"
                ],
                "operation": "aggregate"
              },
              "client.geo.city": {
                "aggregations": [],
                "operation": "groupby"
              },
              "client.geo.country.name": {
                "aggregations": [],
                "operation": "groupby"
              },
              "client.geo.lat": {
                "aggregations": [],
                "operation": "groupby"
              },
              "client.geo.lon": {
                "aggregations": [],
                "operation": "groupby"
              },
              "http.client_ip": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        },
        {
          "id": "convertFieldType",
          "options": {
            "conversions": [
              {
                "destinationType": "number",
                "targetField": "client.geo.lat"
              },
              {
                "destinationType": "number",
                "targetField": "client.geo.lon"
              }
            ],
            "fields": {}
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Max Latency (ms)",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "Duration (max)"
                }
              },
              "operator": "/",
              "right": {
                "fixed": "1000000"
              }
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Duration (max)": true
            },
            "includeByName": {},
            "indexByName": {
              "Duration": 5,
              "client.geo.city": 2,
              "client.geo.country.name": 1,
              "client.geo.lat": 3,
              "client.geo.lon": 4,
              "http.client_ip": 0
            },
            "renameByName": {
              "Duration (max)": "",
              "Max Latency (ms)": "Max Latency (ms)",
              "client.geo.city": "City",
              "client.geo.country.name": "Country",
              "client.geo.lat": "Latitude",
              "client.geo.lon": "Longitude",
              "http.client_ip": "Client IP"
            }
          }
        }
      ],
